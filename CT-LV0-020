def solution(x1, x2, x3, x4):
    answer = True
    y1 = True
    y2 = True
    
    if x1 == x2:
        y1 = x1
    else:
        y1 = True
    
    if x3 == x4:
        y2 = x3
    else:
        y2 = True
        
    if y1 == y2:
        answer = y1
    else:
        answer = False
    return answer
